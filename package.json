{
  "name": "node-18-typescript",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "bin": {
    "classwize-feature-flag": "./dist/index.js"
  },
  "scripts": {
    "dev": "ts-node --esm src/index.ts",
    "test": "NODE_NO_WARNINGS=1 NODE_ENV=test ava"
  },
  "dependencies": {
    "dedent-js": "^1.0.1",
    "dotenv": "^16.3.1",
    "ky": "^1.0.1",
    "radash": "^11.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^18.17.15",
    "@types/yargs": "^17.0.24",
    "ava": "^5.3.1",
    "testdouble": "^3.18.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--loader=testdouble"
    ]
  }
}
